{
    "manifest_version": 3,
    "name": "Pic Locker",
    "description": "Most secure passwords ever",
    "permissions": ["scripting", "activeTab"
  ],
    "version": "1.0",
    "action": {
      "default_popup": "menu.html",
      "default_icon": "logo.png"
    },
    "content_scripts": [{
      "js": ["JS/main.js", "JS/Libaries/steganography.js"], 
      "css": ["CSS/Content.css"],
      "matches": ["https://*/*"]
     }],
     
     "web_accessible_resources": [
      {
        "resources": ["logo.png"], 
        "matches": ["http://*/*", "https://*/*"]
      }

    ]
    
  
    
  }
  